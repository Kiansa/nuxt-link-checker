{
  "name": "nuxt-link-checker",
  "type": "module",
  "version": "2.1.5",
  "packageManager": "pnpm@8.7.0",
  "license": "MIT",
  "funding": "https://github.com/sponsors/harlan-zw",
  "homepage": "https://github.com/harlan-zw/nuxt-link-checker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlan-zw/nuxt-link-checker.git"
  },
  "bugs": {
    "url": "https://github.com/harlan-zw/nuxt-link-checker/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "build": {
    "externals": [
      "ofetch",
      "std-env"
    ]
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build && npm run client:build",
    "build": "npm run prepack",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "dev": "npm run play:dev",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare client",
    "play:dev": "nuxi dev playground",
    "play:prod": "npm run prepack && nuxi dev playground",
    "release": "bumpp package.json --commit --push --tag",
    "lint": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^0.8.2",
    "@nuxt/devtools-ui-kit": "^0.8.2",
    "@nuxt/kit": "^3.7.0",
    "@vueuse/core": "^10.4.1",
    "chalk": "^5.3.0",
    "cheerio": "1.0.0-rc.12",
    "diff": "^5.1.0",
    "fuse.js": "^6.6.2",
    "magic-string": "^0.30.3",
    "nuxt-site-config": "1.0.11",
    "nuxt-site-config-kit": "1.0.11",
    "pathe": "^1.1.1",
    "radix3": "^1.1.0",
    "shiki-es": "^0.14.0",
    "sirv": "^2.0.3",
    "site-config-stack": "^1.0.11",
    "ufo": "^1.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.41.0",
    "@nuxt/module-builder": "^0.5.0",
    "@nuxt/test-utils": "^3.7.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/diff": "^5.0.3",
    "bumpp": "^9.2.0",
    "eslint": "8.48.0",
    "execa": "^8.0.1",
    "nuxt": "^3.7.0",
    "vitest": "^0.34.3"
  }
}
